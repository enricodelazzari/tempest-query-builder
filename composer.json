{
    "name": "enricodelazzari/tempest-query-builder",
    "description": "This is my package tempest-query-builder",
    "keywords": [
        "enricodelazzari",
        "tempest-query-builder"
    ],
    "homepage": "https://github.com/enricodelazzari/tempest-query-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Enrico De Lazzari",
            "email": "enricodlz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "EnricoDeLazzari\\QueryBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnricoDeLazzari\\QueryBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
